- if developer_signed_in?
  %h3 Game Applications
  %ul.client_applications
    - current_game_applications.each do |game_application|
      %li.client_application= link_to game_application.name, edit_dev_game_application_path(game_application)

  = link_to 'add new game', new_dev_game_application_path, :class => 'button'

  %h3 Education Applications
  %ul.client_applications
    - current_education_applications.each do |education_application|
      %li.client_application= link_to education_application.name, edit_dev_education_application_path(education_application)

  = link_to 'add new education', new_dev_education_application_path, :class => 'button'
